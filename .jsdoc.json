{
  "source": {
    "include": ["src", "node_modules/fonero-base/src"],
    "exclude": "node_modules/fonero-base/src/generated"
  },
  "opts": {
    "destination": "./jsdoc/",
    "recurse": true,
    "template": "node_modules/minami",
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ]
}
